@model IEnumerable<Ecommerce.Models.Produtos.Produto>
<h5>Listar Produto</h5>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">NOME</th>
            <th scope="col">PRECO</th>
            <th scope="col">QUANTIDADE</th>
            <th scope="col">QUANTIDADE</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var produto in Model)
        {
            <tr>
                <th scope="row">@produto.Id</th>
                <td>@produto.Nome</td>
                <td>@produto.Preco</td>
                <td>@produto.Quantidade</td>
            </tr>
        }
    </tbody>
</table>